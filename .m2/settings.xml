<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<servers>
		<server>
			<id>open.nexus</id>
			<username>${SERVICE_LOGIN}</username>
			<password>${SERVICE_PWD}</password>
		</server>
	</servers>

	<mirrors>
		<!--Send all requests to the public group -->
		<mirror>
			<id>open.nexus</id>
			<mirrorOf>*,!open.nexus</mirrorOf>
			<url>${MVN_GROUP}</url>
		</mirror>
	</mirrors>
</settings>
